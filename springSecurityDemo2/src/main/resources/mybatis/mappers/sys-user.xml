<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.navy.mapper.sys.SysUserMapper">

	<select id="findByPK" parameterType="java.lang.Long" resultType="sysUser">
		SELECT * FROM T_SYS_USER where id=#{value}
	</select>
	
	<select id="findByUserName" parameterType="java.lang.String" resultType="sysUser">
		SELECT * FROM T_SYS_USER where user_name=#{value}
	</select>
	
	<insert id="insert" parameterType="sysUser">
	insert into T_SYS_USER (
		id,
		user_name,
		password)
	values (
		#{id,jdbcType=BIGINT}, 
		#{username,jdbcType=VARCHAR},
		#{password,jdbcType=VARCHAR})
  </insert>
	
</mapper>